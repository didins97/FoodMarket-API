{
  "name": "foodmarket-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npx jest --runInBand --verbose",
    "make:model": "npx sequelize-cli model:generate --name",
    "make:migration": "npx sequelize-cli migration:generate --name",
    "make:seeder": "npx sequelize-cli seed:generate --name",
    "run:migration": "npx sequelize-cli db:migrate",
    "run:seeder": "npx sequelize-cli db:seed:all",
    "undo:migration": "npx sequelize-cli db:migrate:undo",
    "undo:migration:all": "npx sequelize-cli db:migrate:undo:all",
    "undo:seeder": "npx sequelize-cli db:seed:undo:all",
    "status:migration": "npx sequelize-cli db:migrate:status",
    "start": "nodemon src/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "dotenv": "^16.4.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  }
}
